{
  "name": "ministryofjustice/phpunit-memory-usage",
  "description": "Report high memory usage PHPUnit tests",
  "keywords": [
    "PHPUnit",
    "memory"
  ],
  "homepage": "https://github.com/ministryofjustice/phpunit-memory-usage",
  "license": "MIT",
  "authors": [
    {
      "name": "Ministry of Justice"
    }
  ],
  "require": {
    "php": ">=8.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "MinistryOfJustice\\PHPUnit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MinistryOfJustice\\PHPUnit\\Tests\\": "tests/"
    }
  }
}
